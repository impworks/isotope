{
  "compilerOptions": {
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "experimentalDecorators": true,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "esModuleInterop": true,
    "target": "es5",
    "module": "commonjs",
    "lib": ["es5", "dom", "es2015"]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
