{
  "name": "isotope",
  "version": "1.0.0",
  "description": "A simple photo gallery",
  "type": "module",
  "scripts": {
    "install-all": "cd Areas/Admin/ClientApp && npm install && cd ../../Front/ClientApp && npm install",
    "build-admin": "cd Areas/Admin/ClientApp && npm run build",
    "build-front": "cd Areas/Front/ClientApp && npm run build",
    "dev-admin": "cd Areas/Admin/ClientApp && npm run dev",
    "dev-front": "cd Areas/Front/ClientApp && npm run dev",
    "build": "npm run build-admin && npm run build-front",
    "clean": "rmdir \"wwwroot/@assets\" /s /q"
  },
  "keywords": [
    "photo",
    "gallery"
  ],
  "author": "Impworks",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/impworks/isotope/issues"
  },
  "homepage": "https://github.com/impworks/isotope#readme",
  "-vs-binding": {
    "ProjectOpened": [
      "dev-admin",
      "dev-front"
    ]
  }
}
