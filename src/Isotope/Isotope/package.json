{
  "name": "isotope",
  "version": "1.0.0",
  "description": "A simple photo gallery",
  "type": "module",
  "scripts": {
    "build-admin": "vite build --config vite-admin.config.ts",
    "dev-admin": "vite build --config vite-admin.config.ts -w --sourcemap true",
    "build-front": "vite build --config vite-front.config.ts",
    "dev-front": "vite build --config vite-front.config.ts -w --sourcemap true",
    "build": "npm run build-admin && npm run build-front",
    "clean": "rmdir \"wwwroot/@assets\" /s /q"
  },
  "keywords": [
    "photo",
    "gallery"
  ],
  "author": "Impworks",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/impworks/isotope/issues"
  },
  "homepage": "https://github.com/impworks/isotope#readme",
  "dependencies": {
    "@sum.cumo/vue-datepicker": "2.1.2",
    "@types/hammerjs": "2.0.36",
    "@types/lodash.clonedeep": "4.5.6",
    "@types/luxon": "1.25.0",
    "@types/toastr": "2.1.38",
    "@types/vue2-hammer": "2.1.1",
    "axios": "0.18.0",
    "bootstrap": "4.6.2",
    "font-awesome": "4.7.0",
    "jquery": "3.4.0",
    "lodash": "4.17.20",
    "lodash-decorators": "6.0.1",
    "lodash.clonedeep": "4.5.0",
    "luxon": "1.25.0",
    "popper.js": "1.16.1",
    "portal-vue": "2.1.7",
    "simplebar": "5.3.0",
    "simplebar-vue": "1.6.0",
    "toastr": "2.1.4",
    "vue": "2.7.8",
    "vue-class-component": "7.2.6",
    "vue-draggable-resizable": "2.2.0",
    "vue-fragment": "1.5.1",
    "vue-global-events": "1.2.1",
    "vue-hot-reload-api": "2.3.1",
    "vue-inject": "2.1.1",
    "vue-modal-dialogs": "3.0.0",
    "vue-property-decorator": "9.1.2",
    "vue-router": "3.5.4",
    "vue-select": "3.20.0",
    "vue2-hammer": "2.1.2",
    "vuedraggable": "2.24.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@vitejs/plugin-vue2": "^2.3.1",
    "vite": "^5.1.3"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "dev-admin",
      "dev-front"
    ]
  }
}
